{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "no-shadow": "off",
        "no-param-reassign": "off",
        "eol-last": "off",
        "arrow-parens": "off",
        "no-tabs": 0,
        "no-unused-vars": "off",
        "import/prefer-default-export": "off",
        "no-case-declarations": "off",
        "no-use-before-define": "off",
        "default-case": "off",
        "no-undef": "off",
        "import/no-cycle": "off",
        "consistent-return": "off",
        "no-const-assign": "off"
    },
    "ignorePatterns": [
        "dist/",
        "build/"
    ]
}